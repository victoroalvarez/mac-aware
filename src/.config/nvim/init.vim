" hi Normal ctermbg=Black
colorscheme ron
noremap <Down> <Nop>
noremap <Left> <Nop>
noremap <Right> Nop>
noremap <Up> <Nop>
set autoindent
set clipboard+=unnamedplus
set clipboard+=unnamedplus
set expandtab
set noswapfile
set number
set shiftwidth=4
set softtabstop=4
set tabstop=4
set undodir=$HOME/nvim/undo
set undofile
set undolevels=10000
set undoreload=10000
syntax enable
