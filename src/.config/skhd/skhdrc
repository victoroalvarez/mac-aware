# start kitty terminal
shift + alt - return : open -na kitty

# start apps
#shift + alt - n : open -na "Notes"
shift + alt - 0x2A : open -R '/Users/voa/'
shift + alt - b : open -na "Safari"
shift + alt - c : open -na "Google Chrome"
shift + alt - n : open -na "Google Chrome" --args --new-window "https://keep.google.com"
shift + alt - o : open -na "Obsidian"
shift + alt - s : open -na "Settings"
shift + alt - v : open -na "Vivaldi"
shift + alt - w : open -na "WhatsApp"
#shift + alt - c : open -na "FireFox"
